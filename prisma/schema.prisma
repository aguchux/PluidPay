
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}


// Error Logging
model ErrorLog {
  id         String   @id @default(cuid())
  message    String
  priority   Int   @default(0)
  errorType  String?
  stackTrace String?
  createdAt  DateTime @default(now())
}
